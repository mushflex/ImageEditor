ta(vm.canvas.width, vm.canvas.height);\n      newImgData.data.set(originalData);\n      return newImgData;\n    },\n    adjustBrightness: function adjustBrightness(event) {\n      var vm = this;\n      var newImgData = vm.copyImageData();\n      var data = newImgData.data;\n      var brightnessValue = parseFloat(event.target.value) || 0;\n\n      for (var i = 0; i < data.length; i += 4) {\n        data[i] += brightnessValue;\n        data[i + 1] += brightnessValue;\n        data[i + 2] += brightnessValue;\n      }\n\n      vm.ctx.putImageData(newImgData, 0, 0);\n    },\n    adjustContrast: function adjustContrast(event) {\n      var vm = this;\n      var contrastValue = event.target.value;\n      var newImgData = vm.copyImageData();\n      var data = newImgData.data;\n      contrastValue *= 2.55; // or *= 255 / 100; scale integer percent to full range\n\n      var factor = (255 + contrastValue) / (255.01 - contrastValue); //add .1 to avoid /0 error\n\n      for (var i = 0; i < data.length; i += 4) {\n        data[i] = factor * (data[i] - 128) + 128; //r value\n\n        data[i + 1] = factor * (data[i + 1] - 128) + 128; //g value\n\n        data[i + 2] = factor * (data[i + 2] - 128) + 128; //b value\n      }\n\n      vm.ctx.putImageData(newImgData, 0, 0);\n    },\n    triggerInput: function triggerInput(event) {\n      event.preventDefault();\n      this.$refs.fileInput.click();\n    },\n    onFileSelected: function onFileSelected(event) {\n      var vm = this;\n      var input = event.target;\n\n      if (input.files && input.files[0]) {\n        var reader = new FileReader();\n        var filename = input.files[0].name;\n\n        reader.onload = function (e) {\n          vm.img.src = e.target.result;\n        };\n\n        reader.readAsDataURL(input.files[0]);\n\n        vm.img.onload = function () {\n          vm.ctx.drawImage(vm.img, 0, 0, vm.img.width, vm.img.height, 0, 0, vm.canvas.width, vm.canvas.height);\n          vm.filename = filename;\n          vm.originalImgData = vm.ctx.getImageData(0, 0, vm.canvas.width, vm.canvas.height);\n          vm.isImageLoaded = true;\n        };\n      }\n    }\n  }\n};",{"version":3,"sources":["ImageEditor.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsEA,OAAA,KAAA,MAAA,oBAAA;AACA,OAAA,MAAA,MAAA,qBAAA;AAEA,eAAA;AACA,QAAA,aADA;AAEA,cAAA;AACA,gBADA;AAEA;AAFA,GAFA;AAMA,SAAA,CAAA,IAAA,EAAA,OAAA,EAAA,QAAA,EAAA,UAAA,CANA;AAOA,MAPA,kBAOA;AACA,WAAA;AACA,oBAAA,IADA;AAEA,qBAAA,KAFA;AAGA;AACA,WAAA,IAJA;AAKA,cAAA,IALA;AAMA,WAAA,IANA;AAOA,uBAAA;AAPA,KAAA;AASA,GAjBA;AAkBA,WAAA,mBAAA;AACA;AACA,SAAA,MAAA,GAAA,KAAA,KAAA,CAAA,MAAA;AACA,SAAA,GAAA,GAAA,KAAA,MAAA,CAAA,UAAA,CAAA,IAAA,CAAA;AACA,SAAA,GAAA,GAAA,SAAA,aAAA,CAAA,KAAA,CAAA;AACA,SAAA,QAAA,GAAA,SAAA;AACA,GAxBA;AAyBA,WAAA;AACA,iBADA,2BACA;AACA,UAAA,KAAA,IAAA;AACA,UAAA,eAAA,GAAA,eAAA,CAAA,IAAA;AACA,UAAA,aAAA,GAAA,GAAA,CAAA,eAAA,CAAA,GAAA,MAAA,CAAA,KAAA,EAAA,GAAA,MAAA,CAAA,MAAA,CAAA;AACA,iBAAA,IAAA,CAAA,GAAA,CAAA,YAAA;AACA,aAAA,UAAA;AACA,KAPA;AASA,oBATA,4BASA,KATA,EASA;AACA,UAAA,KAAA,IAAA;AAEA,UAAA,aAAA,GAAA,aAAA,EAAA;AACA,UAAA,OAAA,WAAA,IAAA;AACA,UAAA,kBAAA,WAAA,MAAA,MAAA,CAAA,KAAA,KAAA,CAAA;;AAEA,WAAA,IAAA,IAAA,CAAA,EAAA,IAAA,KAAA,MAAA,EAAA,KAAA,CAAA,EAAA;AACA,aAAA,CAAA,KAAA,eAAA;AACA,aAAA,IAAA,CAAA,KAAA,eAAA;AACA,aAAA,IAAA,CAAA,KAAA,eAAA;AACA;;AACA,SAAA,GAAA,CAAA,YAAA,CAAA,UAAA,EAAA,CAAA,EAAA,CAAA;AACA,KAtBA;AAuBA,kBAvBA,0BAuBA,KAvBA,EAuBA;AACA,UAAA,KAAA,IAAA;AACA,UAAA,gBAAA,MAAA,MAAA,CAAA,KAAA;AACA,UAAA,aAAA,GAAA,aAAA,EAAA;AACA,UAAA,OAAA,WAAA,IAAA;AAEA,uBAAA,IAAA,CANA,CAMA;;AACA,UAAA,SAAA,CAAA,MAAA,aAAA,KAAA,SAAA,aAAA,CAAA,CAPA,CAOA;;AAEA,WAAA,IAAA,IAAA,CAAA,EAAA,IAAA,KAAA,MAAA,EAAA,KAAA,CAAA,EAAA;AACA,aAAA,CAAA,IAAA,UAAA,KAAA,CAAA,IAAA,GAAA,IAAA,GAAA,CADA,CACA;;AACA,aAAA,IAAA,CAAA,IAAA,UAAA,KAAA,IAAA,CAAA,IAAA,GAAA,IAAA,GAAA,CAFA,CAEA;;AACA,aAAA,IAAA,CAAA,IAAA,UAAA,KAAA,IAAA,CAAA,IAAA,GAAA,IAAA,GAAA,CAHA,CAGA;AACA;;AACA,SAAA,GAAA,CAAA,YAAA,CAAA,UAAA,EAAA,CAAA,EAAA,CAAA;AACA,KAtCA;AAuCA,kBAAA,